{
  "name": "next-ts-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "echo ''",
    "dev": "next dev --turbo",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "eslint-check": "eslint --ext .ts,.js,.tsx,.jsx src -f @microsoft/eslint-formatter-sarif -o eslint.sarif",
    "eslint-fix": "eslint --fix --ext .ts,.js,.tsx,.jsx src -f @microsoft/eslint-formatter-sarif -o eslint.sarif",
    "prettier-check": "prettier --check src",
    "prettier-format": "prettier --write src",
    "lint": "yarn prettier-check && yarn eslint-check",
    "lint-fix": "yarn prettier-format && yarn eslint-fix",
    "clean": "rimraf build",
    "pretest": "yarn build",
    "postinstall": "typewind generate",
    "all": "yarn lint && yarn build && yarn test && yarn export"
  },
  "dependencies": {
    "@emotion/react": "*",
    "@emotion/server": "*",
    "@emotion/styled": "*",
    "@mui/icons-material": "*",
    "@mui/material": "*",
    "next": "^13",
    "react": "*",
    "react-dom": "*",
    "typescript": "*",
    "web-vitals": "*"
  },
  "devDependencies": {
    "@microsoft/eslint-formatter-sarif": "*",
    "@next/codemod": "^13",
    "@types/node": "*",
    "@types/react": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "autoprefixer": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-config-airbnb-typescript": "*",
    "eslint-config-import": "*",
    "eslint-config-next": "^13",
    "eslint-config-prettier": "*",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-flowtype": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "next-compose-plugins": "*",
    "next-transpile-modules": "*",
    "postcss": "*",
    "prettier": "*",
    "prettier-plugin-tailwindcss": "*",
    "rimraf": "*",
    "swc-loader": "*",
    "tailwindcss": "*",
    "typewind": "*"
  },
  "packageManager": "yarn@3.5.0"
}
